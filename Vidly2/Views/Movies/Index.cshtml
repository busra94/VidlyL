@model IEnumerable<Vidly2.Models.Movie>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table table-bordered table-hover">
    <tr>  
        <th>Movies</th>
    </tr>
    <tr>
        <td>
            <ul>

                @foreach (var movie in Model)
                {            
                    <li>@movie.Name</li> 
                }

            </ul>
        </td>
    </tr>
</table>

